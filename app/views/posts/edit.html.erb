  <%= form_for(@post) do |f|%>

    <%= f.label :artist %>
    <%= f.text_field :artist %>

    <%= f.label :song, "Track" %>
    <%= f.text_field :song %>

    <%= f.label :img_loc, "Image Location"%>
    <%= f.text_field :img_loc %>

    <%= f.label :player_src, "Player Source" %>
    <%= f.text_field :player_src %>

    <%= f.submit "Post", class: "btn btn-large btn-primary" %>

  <% end %>